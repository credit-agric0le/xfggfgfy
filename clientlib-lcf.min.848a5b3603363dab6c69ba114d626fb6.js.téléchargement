var lclf=function(h,pm,lSession){var cookie_value=null;try{cookie_value=document.cookie}catch(e){}if(cookie_value){var re=new RegExp("(?:^| )("+lSession+"\x3d.[^;]+)","i"),matches=null;var result=null;if(cookie_value.length>0){matches=cookie_value.match(re);if(matches&&matches.length==2)result=matches[1]}}var url="https://"+h;var s=document.createElement("script");s.type="text/javascript";s.async=true;var extra=["dt\x3d"+pm+"\x26r\x3d"+Math.random()];if(result)extra.push(result);s.src=[url,extra.join("\x26")].join("?");
document.getElementsByTagName("head")[0].appendChild(s)};